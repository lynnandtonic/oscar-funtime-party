{
  "name": "oscarfuntime.party",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "2.1.3",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "create-react-app": "^2.1.5",
    "firebase": "^5.8.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-bundler": "^23.0.2",
    "redux-bundler-react": "^1.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "mkdir -p build/css && stylus < src/css/main.styl -c -I src/css 1> build/css/main.css",
    "start-css": "mkdir -p public/css && stylus --sourcemap --watch src/css/main.styl --out public/css/main.css",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "clean": "rm -rf build",
    "start": "npm-run-all -p start-css start-js",
    "build": "npm-run-all clean build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "oscarfuntime.party ======",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lynnandtonic/oscar-funtime-party.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lynnandtonic/oscar-funtime-party/issues"
  },
  "homepage": "https://github.com/lynnandtonic/oscar-funtime-party#readme"
}
